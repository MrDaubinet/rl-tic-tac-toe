{
  "name": "trade-secure-demo",
  "description": "Demo app for TradeSecure",
  "version": "0.0.1",
  "scripts": {
    "build:tailwind": "tailwindcss build -o src/tailwind.css",
    "predev": "npm run build:tailwind",
    "dev": "sapper dev",
    "prebuild": "cross-env NODE_ENV=production npm run build:tailwind",
    "build": "cross-env NODE_ENV=production sapper build --legacy",
    "export": "cross-env NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "@rollup/plugin-url": "^6.0.0",
    "autoprefixer": "^10.2.1",
    "compression": "^1.7.1",
    "moment": "^2.29.1",
    "polka": "next",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sirv": "^0.4.6",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tailwindcss/ui": "^0.5.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/cookie": "^0.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "cross-env": "^7.0.3",
    "ngrok": "^3.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "rollup": "^2.36.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.28.10",
    "svelte": "^3.31.2",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.3.0",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.1.0",
    "typescript": "^4.0.3"
  }
}
