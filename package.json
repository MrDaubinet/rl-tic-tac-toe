    "@types/node": "^13.13.15",
    "cross-env": "^7.0.2",
    "ngrok": "^3.2.7",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.5",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "sapper": "^0.27.16",
    "svelte": "^3.24.1",
    "svelte-preprocess": "^3.9.12",
    "tailwindcss": "^1.7.2",
    "typescript": "^3.9.7"
  }
